                          "type" : "integer",
                          "minimum" : 108,
                          "maximum" : 192,
                          "description" : "Time in hours of insemination to picture.",
                          "example" : 110
                        },
                        "image" : {
                          "type" : "string",
                          "description" : "URL to micrograph or video. This URL will be used by the API to download the image or video to the server.",
                          "example" : "https://myserver.com/embryoimages/embryo1.png"
                        },
