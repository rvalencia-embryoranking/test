                          "type" : "object",
                          "description" : "If PGT was conducted specify the results.",
                          "properties" : {
                            "type_study" : {
                              "type" : "string",
                              "description" : "Indicate the type of study. Valid values: invasive or non-invasive. This parameter is not case sensitive",
                              "example" : "invasive"
                            },
                            "result" : {
                              "type" : "string",
                              "description" : "Result of the study. Valid values: euploid, aneuploid, mosaic, no-dna. This parameter is not case sensitive",
                              "example" : "euploid"
